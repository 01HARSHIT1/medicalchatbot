{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.py": {
      "maxDuration": 30,
      "memory": 3008
    }
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "installCommand": "rm -f requirements.txt requirements-backend-only.txt 2>/dev/null; npm install"
}
