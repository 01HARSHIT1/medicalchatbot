{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.py": {
      "maxDuration": 30,
      "memory": 3008,
      "runtime": "python3.9",
      "includeFiles": "backend-api/datasets/**"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install"
}
